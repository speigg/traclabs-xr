{
    "name": "traclabs-xr",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prebuild": "rimraf dist && rimraf docs",
        "build": "parcel build src/index.html -d docs --public-url /traclabs-xr/",
        "start": "npm run prebuild && parcel src/index.html -d docs --host 0.0.0.0"
    },
    "staticFiles": {
        "staticPath": "src/static",
        "watcherGlob": "**"
    },
    "dependencies": {
        "@types/cannon": "^0.1.3",
        "cannon": "^0.6.2",
        "ethereal": "^1.0.5",
        "three": "^0.110.0",
        "three-mesh-bvh": "^0.1.3",
        "three-stl-loader": "^1.0.6",
        "three-web-layer": "^2.0.5",
        "vue": "^2.6.10",
        "vue-hot-reload-api": "^2.3.4",
        "yeux": "^1.0.1"
    },
    "devDependencies": {
        "@vue/component-compiler-utils": "^3.0.2",
        "autoprefixer": "^9.7.1",
        "parcel": "^1.11.0",
        "parcel-plugin-static-files-copy": "^2.2.1",
        "rimraf": "^2.6.2",
        "typescript": "^3.7.2",
        "vue-template-compiler": "^2.6.10"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "last 2 ios versions",
        "last 2 chrome versions",
        "last 2 firefox versions"
    ]
}
